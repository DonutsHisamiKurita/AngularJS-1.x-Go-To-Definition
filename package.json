{
  "name": "angularjs-1x-go-to-definition",
  "displayName": "AngularJS 1.x Go To Definition ðŸš€",
  "description": "This extension improves code navigation for AngularJS 1-series projects that do not use the module bundler in Visual Studio Code. Allows you to quickly jump from a string-based dependency injection (`MyService`) or a method call (`MyService.doSomething()`) of a service factory controller split into a separate file to its **definition source.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.101.0",
    "node": "^22.15.0"
  },
  "categories": [
    "Programming Languages",
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/DonutsHisamiKurita/AngularJS-1.x-Go-To-Definition.git"
  },
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "angularjs-goto-service.goToServiceDefinition",
        "title": "AngularJS Go To Definition ðŸš€"
      }
    ],
    "keybindings": [
      {
        "command": "angularjs-goto-service.goToServiceDefinition",
        "key": "alt+g",
        "when": "editorTextFocus"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "angularjs-goto-service.goToServiceDefinition",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.101.0",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "esbuild": "^0.25.3",
    "eslint": "^9.25.1",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.8.3",
    "vsce": "^2.15.0"
  }
}
